{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "webpack-dev-server --config webpack.dev.js",
    "build": "vue-cli-service build",
    "test": "jest --no-cache"
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-link-http": "^1.5.16",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "vue": "^2.6.10",
    "vue-apollo": "^3.0.2",
    "vue-drag-drop": "^1.1.4"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.8.4",
    "babel-jest": "^25.1.0",
    "css-loader": "^3.4.2",
    "jest": "^25.1.0",
    "vue-loader": "^15.8.3",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1",
    "webpack-merge": "^4.2.2"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/src/*.js",
      "!**/node_modules/**",
      "!**/coverage/**"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/index.js"
    ],
    "transform": {
      "^.+\\.js": "<rootDir>/node_modules/babel-jest"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/"
    ]
  }
}
